#include <stdio.h>
#include <math.h>

float func(float x);
    
int main()
{
    float x;
    if(scanf("%f", &x) == 1){
        printf("%.1f\n", func(x));
    } else {
        printf("n/a\n");
    }
    
}

float func(float x){
    double y = 7e-3 * pow(x,4) + ((22.8 * pow(x, 1/3) - 1e3) * x + 3) / (x * x / 2) - x * pow((10 + x), (2/x)) - 1.01;
    return round(y * 10) / 10;
}
