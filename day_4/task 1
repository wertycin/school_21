#include <stdio.h>

int is_prime(int a);
int divided_entirely(int a, int b);
int greatest_divider(int a);

int main()
{
    int x;
    float x1;
    if (scanf("%f", &x1) != 1) {
        printf("n/a\n");
    } else {
        x = (int) x1;
        if (x == x1) {
            if ((x > 0) && (x != 1)) {
                printf("%d\n", greatest_divider(x));
            } else if ((x == 0) || (x == 1) || (x == -1)) {
                printf("n/a\n");
            } else {
                printf("%d\n", greatest_divider(-x));
            }
        } else {
            printf("n/a\n");
        }
    }
    return 0;
}

int is_prime(int a){
    int i = 2;
    while (a > i*i - 1){
        if (divided_entirely(a, i)){
            return 0;
        }
        i = i + 1;
    }
    if (a == 1){
        return 0;
    }
    return 1;
}

int divided_entirely(int a, int b){
    while (a >= b){
        a = a - b;
        if (a == 0){
            return 1;
        }
    }
    return 0;
}

int greatest_divider(int a){
    int i = 1;
    int p = 1;
    while (a >= i){
        if (divided_entirely(a, i)){
            if (is_prime(i)){
                p = i;
            }
        }
        i = i + 1;
    }
    return p;
}
